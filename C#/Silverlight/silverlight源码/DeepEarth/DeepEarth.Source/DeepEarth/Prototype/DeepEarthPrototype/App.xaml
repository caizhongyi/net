<Application x:Class="DeepEarthPrototype.App"  
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:DeepProtoControls="clr-namespace:DeepEarthPrototype.Controls"
             xmlns:vsm="clr-namespace:System.Windows;assembly=System.Windows"
             xmlns:Controls="clr-namespace:DeepEarth.Controls;assembly=DeepEarth"
             xmlns:DeepEarth="clr-namespace:DeepEarth;assembly=DeepEarth"
             xmlns:DeepGeometry="clr-namespace:DeepEarth.Geometry;assembly=DeepEarth"
             mc:Ignorable="d" >

    <Application.Resources>


        <!--Style Map-->
        <Style x:Name="PrototypeMapStyle" TargetType="DeepEarth:Map">
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="DeepEarth:Map">
                        <Grid>
                            <Grid x:Name="PART_LayoutRoot">
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="*" />
                                    <RowDefinition Height="Auto" />
                                </Grid.RowDefinitions>
                                <Grid x:Name="PART_MapContent" Grid.RowSpan="2" RenderTransformOrigin="0.5,0.5">
                                    <Grid.RenderTransform>
                                        <RotateTransform x:Name="PART_MapRotation" />
                                    </Grid.RenderTransform>
                                    <Controls:MouseControl x:Name="PART_MouseControl"/>
                                    <Grid x:Name="PART_MapLayers" />
                                </Grid>
                                <StackPanel x:Name="PART_LicenseContainer" Orientation="Horizontal" VerticalAlignment="Bottom" HorizontalAlignment="Left" />
                            </Grid>
                            <StackPanel HorizontalAlignment="Right">
                                <HyperlinkButton FontSize="14" NavigateUri="http://www.codeplex.com/deepearth/" ToolTipService.ToolTip="Interested in Mapping and Silverlight Deep Zoom? Click here.">
                                    <Image Margin="8" Source="DeepEarthMap.png" VerticalAlignment="Top" HorizontalAlignment="Right" Stretch="None" />
                                </HyperlinkButton>
                            </StackPanel>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style>

