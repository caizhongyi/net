import org.swxformat.miniflickr.ThumbScreen;import org.swxformat.miniflickr.DetailScreen;class org.swxformat.miniflickr.Application extends MovieClip{	// 	// Properties	//	private var thumbScreen:ThumbScreen;	private var detailScreen:DetailScreen;	//	// Event handlers	//		function Application()	{		_level0._focusrect = false;		fscommand2("SetSoftKeys", "Back", "Exit");						// Listen for key events.		Key.addListener(this);			}		private function onLoad():Void	{		// Full screen		fscommand2("FullScreen", true);				thumbScreen.visible = true;		detailScreen.visible = false;		thumbScreen.addEventListener(ThumbScreen.GET_PHOTO_DETAILS, this);		detailScreen.addEventListener(DetailScreen.CLOSE_DETAILS, this);			}	private function getPhotoDetails(event:Object):Void	{		var photoId:String = event.id;		var photoIds:Array = event.ids;		thumbScreen.visible = false;				// Find the index of the photo id in the ids and pass that to the detail screen		var photoIndex:Number = null;		for (var i = 0; i < photoIds.length; i++)		{			if (photoIds[i] == photoId)			{				photoIndex = i;				break;			}		}		detailScreen.showPicture(photoIds, photoIndex);	}	private function closeDetails(event:Object):Void	{		detailScreen.visible = false;		thumbScreen.visible = true;	}	// Main key handling	private function onKeyUp ():Void	{		var code = Key.getCode();		switch(code) 		{			case ExtendedKey.SOFT2:				// Exit				fscommand2("Quit");			break;		}	}}