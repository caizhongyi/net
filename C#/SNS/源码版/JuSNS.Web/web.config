<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="WebDAL" value="JuSNS.SQLServer" />
  </appSettings>
  <connectionStrings>
    <add name="snsconstr" connectionString="server=.\sqlexpress;uid=sa;pwd=sa;database=newsns;Pooling=true;" />
  </connectionStrings>
  <system.web>
    <httpHandlers>
      <!--<add verb="*" path="*.html" type="JuSNS.MVC.IHandler, JuSNS.MVC" />
      <add verb="*" path="*.html" type="JuSNS.MVC.IHandler, JuSNS.MVC" />-->
      <add verb="*" path="*.aspx" type="JuSNS.MVC.IHandler, JuSNS.MVC" />
    </httpHandlers>
    <httpRuntime useFullyQualifiedRedirectUrl="true" maxRequestLength="51400" executionTimeout="60" />
    <globalization requestEncoding="utf-8" responseEncoding="utf-8" />
    <compilation debug="true">
      <assemblies>
        <add assembly="System.Windows.Forms, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
        <add assembly="System.Design, Version=2.0.0.0, Culture=neutral, PublicKeyToken=B03F5F7F11D50A3A" />
      </assemblies>
    </compilation>
    <pages>
      <!--<controls>
        <add tagPrefix="asp" namespace="System.Web.UI" assembly="System.Web.Extensions, Version=1.0.61025.0, Culture=neutral, PublicKeyToken=31bf3856ad364e35" />
        <add tagPrefix="jusns" namespace="JuSNS.Control" assembly="JuSNS.Control" />
        <add tagPrefix="jusns" namespace="JuSNS.Control.Validator" assembly="JuSNS.Control" />
      </controls>-->
    </pages>
    <authentication mode="Windows" />
    <xhtmlConformance mode="Legacy" />
  </system.web>
  <system.webServer>
    <validation validateIntegratedModeConfiguration="false" />
    <defaultDocument>
      <files>
        <clear />
        <add value="index.htm" />
        <add value="index.html" />
        <add value="default.aspx" />
      </files>
    </defaultDocument>
    <!--For IIS 7.0+-->
    <handlers>
      <!--<add name="JuSNSHTML" path="*.html" verb="*" type="JuSNS.MVC.IHandler, JuSNS.MVC" preCondition="integratedMode,runtimeVersionv2.0" />-->
      <add name="JuSNSASPX" path="*.aspx" verb="*" type="JuSNS.MVC.IHandler, JuSNS.MVC" preCondition="integratedMode,runtimeVersionv2.0" />
    </handlers>
    
   <!-- IIS 7.0+ 在IIS里安装了URLWiter后，可以解除此屏蔽，并且在各个配置文件中开启设置URL伪静态
    <rewrite>
      <rules>
        <rule name="Group">
          <match url="^info/group-([0-9]+)\.html" negate="false" />
          <action type="Rewrite" url="/app/group/group.html?gid={R:1}" />
        </rule>
        <rule name="Topic">
          <match url="^info/topic-([0-9]+)\.html" />
          <action type="Rewrite" url="/app/group/topic.html?tid={R:1}" />
        </rule>
        <rule name="News">
          <match url="^info/news-([0-9]+)\.html" />
          <action type="Rewrite" url="/app/news/info.html?nid={R:1}" />
        </rule>
        <rule name="Blog">
          <match url="^info/blog-([0-9]+)\.html" />
          <action type="Rewrite" url="/app/blog/info.html?bid={R:1}" />
        </rule>
        <rule name="Twitter">
          <match url="^info/twitter-([0-9]+)\.html" />
          <action type="Rewrite" url="/app/twitter/info.html?tid={R:1}" />
        </rule>
        <rule name="User">
          <match url="^user/([0-9]+)" />
          <action type="Rewrite" url="/user/default.html?uid={R:1}" />
        </rule>
        <rule name="Album">
          <match url="^info/album-([0-9]+).html" />
          <action type="Rewrite" url="/app/album/albumview.html?aid={R:1}" />
        </rule>
        <rule name="Photo">
          <match url="^info/photo-([0-9]+).html" />
          <action type="Rewrite" url="/app/album/photoview.html?pid={R:1}" />
        </rule>
        <rule name="Ative">
          <match url="^info/ative-([0-9]+).html" />
          <action type="Rewrite" url="/app/ative/info.html?aid={R:1}" />
        </rule>
        <rule name="Ask">
          <match url="^info/ask-([0-9]+).html" />
          <action type="Rewrite" url="/app/ask/info.html?aid={R:1}" />
        </rule>
        <rule name="Goods">
          <match url="^info/goods-([0-9]+).html" />
          <action type="Rewrite" url="/app/shop/goods.html?gid={R:1}" />
        </rule>
        <rule name="Shop">
          <match url="^shop/([0-9]+)" />
          <action type="Rewrite" url="/user/shop/default.html?sid={R:1}" />
        </rule>
        <rule name="Multe">
          <match url="^info/multe-([0-9]+).html" />
          <action type="Rewrite" url="/app/shop/multe.html?mid={R:1}" />
        </rule>
        <rule name="Vote">
          <match url="^info/vote-([0-9]+).html" />
          <action type="Rewrite" url="/app/vote/view.html?vid={R:1}" />
        </rule>
      </rules>
    </rewrite>-->
  </system.webServer>
</configuration>