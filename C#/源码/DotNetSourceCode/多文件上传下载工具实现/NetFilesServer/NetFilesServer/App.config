<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <!--HFSoft FrameWork配置信息-->
    <section name="hfsoftconfig" type="HFSoft.FrameWorkConfigHandler, HFSoft, Version=1.0.0.0, Culture=neutral, PublicKeyToken=null" />
  </configSections>
  <hfsoftconfig>
    <exceptionhandlers>
      <!--配置异常处理对象-->
      <handler type="NetFiles.Common.ExceptionHandler,NetFiles.Common"/>
    </exceptionhandlers>
    <dataaccess>
      <!--配置数据库处理对象-->
      <DriverType value="HFSoft.Data.OledbAccess, HFSoft.Data, Version=1.3.0.0, Culture=neutral, PublicKeyToken=null"/>
      <ConnectionString  value=""/>
      <MappingAssemblys>
        <Assembly value="NetFiles.DataAccess"/>
      </MappingAssemblys>
    </dataaccess>
  </hfsoftconfig>
  <appSettings>
    <add key="ipaddress" value="192.168.0.229"/>
    <add key="port" value="8086"/>
  </appSettings>
</configuration>